      },
      "collection.features": {
         "begin": "(?i)(features)\\s*(?::=|=|\\s)\\s*(\\{)",
         "end": "\\s*(})\\s*",
         "contentName": "dotnugg.collection.features.details",
         "name": "dotnugg.collection.features",
         "beginCaptures": {
            "0": {
               "name": "dotnugg.collection.features.open"
            },
            "1": {
