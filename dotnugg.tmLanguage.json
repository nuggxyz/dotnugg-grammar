      "general.color": {
         "match": "\\s*(\\w)\\s*(?::=|=|\\s)\\s*(\\s*({)\\s*(zindex|layer|lay)\\s*:\\s*((\\+|-)(\\d+|D))\\s*,\\s*(rgba|color|col)\\s*:\\s*((?:'|\\\")(.*)(?:'|\\\"))\\s*(})\\s*)\\s*",
